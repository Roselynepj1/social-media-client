{
  "name": "css-frameworks-ca",
  "version": "1.0.0",
  "description": "Test",
  "main": "index.js",
  "scripts": {
    "build": "sass src/scss:dist/css",
    "start": "sass --watch src/scss:dist/css & live-server",
    "watch:sass": "sass --watch src/scss:dist/css",
    "start:server": "live-server",
    "dev": "npm-run-all --parallel watch:sass start:server",
    "lint": "eslint .",
    "format": "prettier --write .",
    "lint:format": "npm-run-all --parallel lint format",
    "test": "jest --watch",
    "precommit": "npm run lint && npm run format && npm test",
    "cypress:open": "cypress open",
    "prepare": "husky && husky install"
  },
  "keywords": [],
  "author": "Noroff",
  "license": "ISC",
  "lint-staged": {
    "**/*": [
      "eslint --fix",
      "prettier --write --ignore-unknown"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.24.3",
    "@babel/preset-env": "^7.24.3",
    "babel-jest": "^29.7.0",
    "cypress": "^13.7.1",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-config-standard": "^17.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jest": "^27.9.0",
    "eslint-plugin-n": "^16.6.2",
    "eslint-plugin-promise": "^6.1.1",
    "husky": "^8.0.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "jsdom": "^24.0.0",
    "lint-staged": "^15.2.2",
    "live-server": "^1.2.2",
    "npm-run-all": "^4.1.5",
    "prettier": "3.2.5",
    "sass": "^1.54.8"
  },
  "dependencies": {
    "bootstrap-dark-5": "^1.1.3"
  }
}
